{
    "name": "revolution/laravel-namespaced-helpers",
    "description": "Laravel Namespaced helpers",
    "keywords": [
        "laravel",
        "helpers"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0||^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\Illuminate\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
